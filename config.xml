<?xml version='1.0' encoding='utf-8'?>
<widget id="com.api.aqcua" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Acquavel</name>
    <description>
        Acquavel Unplugged Life.
    </description>
    <author email="marv1336@gmail.com" href="https://github.com/scorpmarv">
        ScorpMarv
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/img/ldpi.png" density="ldpi" />
        <icon src="www/img/mdpi.png" density="mdpi" />
        <icon src="www/img/hdpi.png" density="hdpi" />
        <icon src="www/img/xhdpi.png" density="xhdpi" />
    </platform>
</widget>
